{
  "name": "aoda.css",
  "private": false,
  "version": "1.0.1",
  "license": "MIT",
  "description": "ðŸŽ¨ Lighter and faster atomic stylesheet",
  "keywords": [
    "css",
    "library"
  ],
  "author": "aodazhang <zhangxinyu1989@foxmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/aodazhang/aoda.css.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.css",
  "module": "dist/index.css",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev webpack serve --config ./webpack.config.js",
    "build": "rimraf dist dist-example dist-scss && sass scss:dist-scss --no-source-map --style compressed && cross-env NODE_ENV=prod webpack --config ./webpack.config.js",
    "lint": "stylelint --fix --config stylelint.config.js **/*.scss && prettier --write ./**/*.{scss,js,ts,vue,json,md} && eslint --fix example/**/*.{ts,vue}",
    "commit": "git add . && git-cz",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vue/compiler-sfc": "^3.2.33",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "commitizen": "^4.2.4",
    "compression-webpack-plugin": "^9.2.0",
    "copy-webpack-plugin": "^10.2.4",
    "core-js": "^3.22.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "gsap": "^3.10.4",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.0",
    "lint-staged": "^12.4.1",
    "mini-css-extract-plugin": "^2.6.0",
    "pinia": "^2.0.14",
    "postcss": "^8.4.13",
    "postcss-loader": "^6.2.1",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^2.6.2",
    "register-service-worker": "^1.7.2",
    "rimraf": "^3.0.2",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.8.2",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.6.4",
    "vue": "^3.2.33",
    "vue-eslint-parser": "^8.3.0",
    "vue-loader": "^17.0.0",
    "vue-router": "^4.0.15",
    "webpack": "^5.72.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0",
    "webpack-merge": "^5.8.0",
    "workbox-webpack-plugin": "^6.5.3"
  },
  "lint-staged": {
    "*.scss": [
      "stylelint --fix",
      "git add"
    ],
    "*.{ts,vue}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
